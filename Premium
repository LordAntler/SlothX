
SlothXPremium = {
}
  
  SlothXStaff = {
  }
  
  SlothXOwner = {
  3372205425,
  }
  
  SlothXNigger = {
  3372209077,
  }
function sloths()
	for _,Player in pairs(game:GetService('Players'):GetChildren()) do
		if table.find(SlothXPremium, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == 'Players' then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[ğŸ¦¥PremiumğŸ¦¥]' .. Player.DisplayName)
				end
			end
		elseif
			table.find(SlothXStaff, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == 'Players' then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[ğŸ¦¥StaffğŸ¦¥]' .. Player.DisplayName)
				end
			end
		elseif
			table.find(SlothXOwner, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == 'Players' then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[ğŸ¦¥OwnerğŸ¦¥]' .. Player.DisplayName)
				end
			end
		elseif
			table.find(SlothXOwner, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == 'Players' then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[ğŸ‘¨ğŸ¿NiggerğŸ‘¨ğŸ¿]' .. Player.DisplayName)
				end
			end
		if
			Player.Character then
			if Player.Character.Parent.Name == 'Players' then
				if not Player.Character.UpperTorso:FindFirstChild('OriginalSize') then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[ğŸ¦¥UserğŸ¦¥]' .. Player.DisplayName)
				end
			end
		end
	end
	end
	end
local p,err = pcall(sloths)
return SlothXPremium
